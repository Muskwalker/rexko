<% unless (f.object._destroy) %>
	<% nested_attributes_for f, :senses, :etymotheses, [:notes, :subentry] %>
	<fieldset class="subentry">
	  <legend>
			<%= label_with_remove_option_for "Subentry #{subentry_counter + 1  if defined? subentry_counter}", f %>
		</legend>
		
		<div class="type-text">
	      <%= f.label :paradigm, "Principal parts" %>
	      <%= f.text_field :paradigm %>
	      <%= f.label :part_of_speech %>
	      <%= f.text_field :part_of_speech %>
	  </div>
		<%= f.fields_for :notes do |e_form| %>
	    <%= render :partial => 'note', :locals => { :f => e_form } %>
	  <% end %>
		<div class="par">
			<%= link_to 'Add note', '#notes', :class => 'add_nested', :rel => "[subentry][notes]" %>
		</div>
    
	  <!-- Sense -->
	  <%= list_children_with_option_to_add :sense, f, remote: true, create_blank: true %>

	  <!-- Etymology -->
		<%= list_children_with_option_to_add :etymothesis, f, display_name: 'etymology' %>
	</fieldset>
<% end %>
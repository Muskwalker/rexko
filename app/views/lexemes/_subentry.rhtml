<% unless (f.object._delete) %>
	<% nested_attributes_for f, :senses, :etymologies %>
	<div class="subentry">
	  <h1>Subentry <%= subentry_counter + 1  if defined? subentry_counter  %></h1>
	  <table>
	    <tr>
	      <td><b>Principal parts:</b></td>
	      <td><%= f.text_field :paradigm %></td>
	    </tr>
	    <tr>
	      <td>Part of speech:</b></td>
	      <td><%= f.text_field :part_of_speech %></td>
	    </tr>
	  </table>
    <%= remove_link_unless_new_record(f) %>

	  <!-- Sense -->
	  <% @sense = f.object.senses.empty? ? f.object.senses.build : f.object.senses %>
	  <% f.fields_for :senses do |s_form| %>
	    <%= render :partial => 'sense', :locals => { :f => s_form } %>
	  <% end %>
		<span class="par">
			<%= link_to 'Add sense', '#senses', :class => 'add' %>
		</span>

	  <!-- Etymology -->
	  <h2>Etymology</h2>
	  <table>
	    <tr>
	      <th>Source Language</th>
	      <th>Etymon</th>
	      <th>Gloss</th>
	    </tr>
    
	    <% @etymology = f.object.etymologies.empty? ? f.object.etymologies.build : f.object.etymologies 
	%>
	    <% f.fields_for :etymologies do |e_form| %>
	      <%= render :partial => 'etymology', :locals => { :f => e_form } %>
	    <% end %>
			<tr class="par">
				<td>
					<%= link_to 'Add etymology', '#etymologies', :class => 'add' %>
				</td>
			</tr>
	  </table>
	</div>
<% end %>
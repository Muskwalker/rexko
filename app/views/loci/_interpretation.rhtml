<% unless (f.object._delete) %>
	<span class="interpretation">
		<br><b>Sense:</b> <%= f.collection_select :sense_id, @interpretations ? @interpretations[f.object.parse.parsed_form] : f.object.parse.potential_interpretations, :id, :definition, {:include_blank => true} if f.object.parse %> 

	  <%= remove_link_unless_new_record(f) %>
	</span>
<% end %>